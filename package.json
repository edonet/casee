        "postcss-loader": "^2.0.7",
        "raw-loader": "^0.5.1",
        "react-hot-loader": "^3.1.2",
        "redux-immutable-state-invariant": "^2.1.0",
        "redux-logger": "^3.0.6",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.19.0",
        "svgx-loader": "^0.0.3",
        "url-loader": "^0.6.2",
        "var-loader": "^1.0.202",
        "webpack": "^3.7.1",
        "webpack-dev-server": "^2.9.1"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "testMatch": [
            "<rootDir>/src/**/*.(test|spec).js?(x)"
        ],
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/settings/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/settings/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
        ]
    }
