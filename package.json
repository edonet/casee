        "redux-thunk": "^2.2.0"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.5",
        "babel-core": "^6.26.0",
        "babel-jest": "^21.2.0",
        "babel-loader": "^7.1.2",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-react-app": "^3.1.0",
        "babel-preset-react-optimize": "^1.0.1",
        "babel-preset-stage-3": "^6.24.1",
